.module
	.module-content
		ul.listing
			li.listItem(ng-repeat="official in officials")
				.listItem--content
					.listItem--utils
						button.btn(ng-click="upVote($index, official._id, user.id)")
							i.material-icons keyboard_arrow_up
						span.votes_count {{official.voteCount}}
						button.btn(ng-click="downVote($index, official._id, user.id)")
							i.material-icons keyboard_arrow_down
					a(href="#")
						h4 {{official.fullname}}
					.block
						a.tag(href="#") Partido Verde
						a.tag(href="#") Alcald√≠a
					a.toggleData(href="#")
						i.material-icons more_vert
